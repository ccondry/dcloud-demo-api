[Unit]
Description=proxy for saving chat survey answers into ccedata database

[Service]
ExecStart=/usr/bin/node /opt/dcloud/survey-answers-api/index
Restart=always
User=root
Group=root
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/opt/dcloud/survey-answers-api

[Install]
WantedBy=multi-user.target
